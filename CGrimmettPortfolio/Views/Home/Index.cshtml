                                            <h2 class="text-center">5 Number Function</h2>
                                            <hr class="primary" />
                                            <p>
                                                This function will take the five numbers you input and
                                                give you the Sum, Mean, Product, Lowest and Highest values
                                                on execution.
                                            </p>
                                            <input class="form-control" id="numero uno" type="number" style="margin-bottom:10px;" placeholder="Enter first number" />
                                            <input class="form-control" id="numero dos" type="number" style="margin-bottom:10px;" placeholder="Enter second number" />
                                            <input class="form-control" id="numero tres" type="number" style="margin-bottom:10px;" placeholder="Enter third number" />
                                            <input class="form-control" id="numero quatro" type="number" style="margin-bottom:10px;" placeholder="Enter fourth number" />
                                            <input class="form-control" id="numero cinco" type="number" style="margin-bottom:10px;" placeholder="Enter fifth number" />
                                            <button class="btn btn-primary" onclick="numCalc()">Execute</button>


                                            <div id="Result1">Sum</div>
                                            <div id="Result2">Mean</div>
                                            <div id="Result3">Product</div>
                                            <div id="Result4">Least</div>
                                            <div id="Result5">Most</div>
                                        </div>
                                    </div>
                                </div>
                                <script>
                                    function numCalc() {
                                        var firstNum = Number(document.getElementById("numero uno").value);
                                        var secondNum = Number(document.getElementById("numero dos").value);
                                        var thirdNum = Number(document.getElementById("numero tres").value);
                                        var fourthNum = Number(document.getElementById("numero quatro").value);
                                        var fifthNum = Number(document.getElementById("numero cinco").value);
                                        var resOne = firstNum + secondNum + thirdNum + fourthNum + fifthNum;
                                        document.getElementById("Result1").innerHTML = resOne;

                                        var resTwo = firstNum + secondNum + thirdNum + fourthNum + fifthNum / 5;
                                        document.getElementById("Result2").innerHTML = resTwo;

                                        var resThree = firstNum * secondNum * thirdNum * fourthNum * fifthNum;
                                        document.getElementById("Result3").innerHTML = resThree;
                                        document.getElementById("Result4").innerHTML = Math.min(firstNum,
                                            secondNum, thirdNum, fourthNum, fifthNum);
                                        document.getElementById("Result5").innerHTML = Math.max(firstNum,
                                            secondNum, thirdNum, fourthNum, fifthNum);
                                    }
                                </script>
                                <div class="grid-66">
                                    <pre class="brush: js">
function numCalc() {
   var firstNum = Number(document.getElementById("numero uno").value);
   var secondNum = Number(document.getElementById("numero dos").value);
   var thirdNum = Number(document.getElementById("numero tres").value);
   var fourNum = Number(document.getElementById("numero quatro").value);
   var fifthNum = Number(document.getElementById("numero cinco").value);
   //Sum
   var resOne = firstNum + secondNum + thirdNum + fourNum + fifthNum;
   document.getElementById("Result1").innerHTML = resOne;
   //Mean
   var resTwo = firstNum + secondNum + thirdNum + fourNum + fifthNum / 5;
   document.getElementById("Result2").innerHTML = resTwo;
   //product
   var resThree = firstNum * secondNum * thirdNum * fourNum * fifthNum;
   document.getElementById("Result3").innerHTML = resThree;
   //Min
   document.getElementById("Result4").innerHTML = Math.min(firstNum, 
        secondNum, thirdNum, fourNum, fifthNum);
   //Max
   document.getElementById("Result5").innerHTML = Math.max(firstNum, 
        secondNum, thirdNum, fourNum, fifthNum);
   }
